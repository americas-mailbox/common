{
  "name": "amb/common",
  "description": "AMB common code",
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^2.13.7",
    "league/flysystem": "2.x-dev",
    "iampersistent/ledger": "^0.3.1",
    "iampersistent/simple-shop": "^v0.4.1",
    "myclabs/php-enum": "^1.7",
    "ramsey/uuid": "^4.1",
    "zestic/app-library": "^0.7.0",
    "zestic/communication-component": "^1.1.1",
    "zestic/contracts": "v0.2.0",
    "zestic/flysystem2-component": "^0.1.1"
  },
  "require-dev": {
    "amb/codeception-amb": "^1.6",
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.3"
  },
  "repositories": {
    "amb/codeception-amb": {
      "type": "vcs",
      "url": "git@github.com:americas-mailbox/codeception-amb.git"
    }
  },
  "autoload": {
    "psr-4": {
      "AMB\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Functional\\": "tests/functional",
      "Unit\\": "tests/unit"
    }
  }
}
